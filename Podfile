  pod 'MJRefresh'          #下拉刷新上拉加载
  pod 'MJExtension'         #转模型
  pod 'Masonry'
  pod 'WCDB',git: 'https://github.com/Tencent/wcdb.git' , branch: 'master'  #数据库
  pod 'TTGTagCollectionView'   
end

